# "tester";

let t <- Tester();
t.msg("TESTING FOR-EACH-LOOPS");
say NL;

t.msg("Testing regular for-each");
let l = [1, 2, 3, 4];
let test;
let res = 0;
for (test : l) {
    res += test;
}
t.match(res, 10);

t.msg("Testing for-each in a for-each");
l = [[1, 2, 3], [4, 5, 6], [7, 8, 9]];
let result = 0;
let inner;
for (test : l) {
    for (inner : test) {
        result += inner;
    }
}
t.match(result, 45);

t.msg("Testing map-for-each");
let t = {"key": "val", "name" : "mani", "date" : "today"};
let key;
let val;
let resu = [];
for (key, val : t) {
    res.add(key);
    res.add(val);
}
t.match(resu, ["key", "val", "name", "mani", "date", "today"]);
